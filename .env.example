# ClickUp OAuth Configuration
CLICKUP_CLIENT_ID=your_clickup_client_id_here
CLICKUP_CLIENT_SECRET=your_clickup_client_secret_here
CLICKUP_REDIRECT_URI=http://localhost:8787/api/clickup/auth/callback

# Application Configuration
APP_BASE_URL=http://localhost:8787
APP_ENVIRONMENT=development

# CORS Configuration (comma-separated origins)
CORS_ORIGINS=http://localhost:3000,http://localhost:8080

# Optional: Webhook Configuration
CLICKUP_WEBHOOK_SECRET=your_webhook_secret_here

# Optional: Zendesk Configuration
ZENDESK_API_TOKEN=your_zendesk_api_token_here
ZENDESK_SUBDOMAIN=your_zendesk_subdomain_here

# Security
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here