{
  "name": "zendesk-clickup-automation",
  "version": "0.0.1",
  "description": "AI-powered multi-agent automation for Zendesk tickets with ClickUp task creation and Slack notifications",
  "private": true,
  "keywords": [
    "zendesk", "clickup", "slack", "automation", "ai", "multi-agent", "cloudflare-workers"
  ],
  "scripts": {
    "dev": "wrangler dev",
    "dev:prod": "wrangler dev --config wrangler.production.jsonc",
    "deploy": "wrangler deploy --config wrangler.dev.jsonc",
    "deploy:prod": "wrangler deploy --config wrangler.production.jsonc",
    "build": "wrangler deploy --dry-run --outdir=./dist",
    "check-size": "wrangler deploy --dry-run",
    "cf-typegen": "wrangler types",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@google/generative-ai": "^0.7.1",
    "hono": "^4.6.20"
  },
  "devDependencies": {
    "@types/node": "22.13.0",
    "@types/service-worker-mock": "^2.0.4",
    "typescript": "^5.9.2",
    "wrangler": "^4.28.0"
  }
}
